iew.ts","name":null,"original":{"line":71,"column":11},"generated":{"line":84,"column":13}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":71,"column":16},"generated":{"line":84,"column":18}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":71,"column":11},"generated":{"line":84,"column":25}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":71,"column":4},"generated":{"line":84,"column":27}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":72,"column":3},"generated":{"line":85,"column":0}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":44},"generated":{"line":89,"column":2}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":44},"generated":{"line":89,"column":8}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":44},"generated":{"line":89,"column":9}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":0}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":7}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":8}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":15}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":18}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":32,"column":0},"generated":{"line":91,"column":32}}],"sources":{"src/page/news-detail-view.ts":"import View from '../core/view';\nimport { NewsDetailApi } from '../core/api';\nimport { NewsComment, NewsDetail, NewsStore } from '../types';\nimport { CONTENT_URL } from '../config';\n\nconst template = `\n      <div class =\"bg-gray-600 min-h-screen pb-8\">\n        <div class=\"bg-white text-xl\">\n          <div class=\"mx-auto px-4\">\n            <div class=\"flex justify-between items-center py-6\">\n              <div class=\"flex justify-start\">\n                <h1 class=\"font-extrabold\">Hacker News</h1>\n              </div>\n              <div class=\"flex items-center justify-end\">\n                <a href=\"#/page/{{__currentPage__}}\" class=\"text-gray-500\">\n                  <i class=\"fa fa-times\"></i>\n                </a>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"h-full border rounded-xl bg-white m-6 p-4\">\n          <h2>{{__title__}}</h2>\n          <div class=\"text-gray-400 h-20\">\n            {{__content__}}\n          </div>\n          {{__comments__}}\n        </div>\n      </div>\n    `;\n\nexport default class NewsDetailView extends View {\n  private store: NewsStore;\n  constructor(containerId: string, store: NewsStore) {\n    super(containerId, template);\n    this.store = store;\n  }\n\n  render() {\n    const id = location.hash.substr(7); // substr은 문자열에서 배열은 0부터 시작하고 1을 적었기 때문에 0번째 문자는 짤리고 1~lastNum 까지의 문자열이 반환된다. 즉 #가 짤려나간다.\n    const api = new NewsDetailApi(CONTENT_URL.replace('@id', id));\n    const newsDetail: NewsDetail = api.getData();\n\n    this.store.makeRead(Number(id));\n    this.setTemplateData('currentPage', String(this.store.currentPage));\n    this.setTemplateData('title', newsDetail.title);\n    this.setTemplateData('content', newsDetail.content);\n    this.setTemplateData('comments', this.makeComment(newsDetail.comments));\n    this.updateView();\n  }\n\n  makeComment(comments: NewsComment[]): string {\n    for (let i = 0; i < comments.length; i++) {\n      const comment: NewsComment = comments[i];\n      this.addHtml(`\n        <div style=\"padding-left:${comment.level * 40}px;\" class=\"mt-4\">\n          <div class=\"text-gray-400\">\n            <i class=\"fa fa-sort-up mr-2\"></i>\n            <strong>${comment.user}</strong> ${comment.time_ago}\n          </div>\n          <p class=\"text-gray-700\">${comment.content}</p>\n        </div>\n      `);\n\n      //재귀 호출\n      if (comment.comments.length > 0) {\n        this.addHtml(this.makeComment(comment.comments));\n      }\n    }\n\n    return this.getHtml();\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"9a8c7aa43eec08e51dcc2250ad837dfd","cacheData":{"env":{}}}